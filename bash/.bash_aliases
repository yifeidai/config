# cd aliases
alias codes='cd ~/IdeaProjects'
alias config='cd ~/IdeaProjects/config'
alias hbapp='cd ~/IdeaProjects/hb-app'
alias hbmp='cd ~/IdeaProjects/hb-mp'
alias hbadmin='cd ~/IdeaProjects/hb-admin'

# git aliases
alias ga='git add'
alias ga.='git add .'

alias gb='git branch'

alias gco='git checkout'
alias gcom='git checkout master'
alias gcod='git checkout dev'
alias gcob='git checkout -b'

alias gc='git commit'
alias gcm='git commit -m'
alias gca='git commit -a'
alias gcn='git commit --no-edit --amend'
alias gcv='git commit --no-verify'

alias gd='git diff'

alias gs='git status'

alias glg='git log --graph --oneline'

alias gm='git merge'
alias gmd='git merge dev'
alias gmm='git merge master'

alias gu='git pull'
alias gur='git pull --rebase'
alias gp='git push'
alias gpf!='git push --force'

alias grb='git rebase'
alias grbd='git rebase dev'
alias grbm='git rebase master'
alias grba='git rebase --abort'
alias grbc='git rebase --continue'
alias grbs='git rebase --skip'

alias grs='git reset'
alias grsl='git reset HEAD'
alias grsh!='git reset --hard'

alias gst='git stash'
alias gstp='git stash pop'
alias gstl='git stash list'

# open current folder in terminal
alias explorer="explorer.exe ."
